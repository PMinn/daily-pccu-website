{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "_next/static/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, s-maxage=10, stale-while-revalidate=59"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/course{,/**}",
        "destination": "/course/[[...params]].html"
      },
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "redirects": [],
    "cleanUrls": true
  }
}